<resources xmlns:xliff="http://schemas.android.com/apk/res-auto">
    <string name="app_name">MyWeatherDatabase</string>
    <string name="action_settings">Settings</string>

    <string name="content_authority">com.example.myweatherdatabase</string>




    <!-- - - - - - - - - - - - - - -
  - Used by DateUtils  -
  - - - - - - - - - - - - - - -->

    <!-- For labelling today's forecast -->
    <string name="today">Today</string>

    <!-- For labelling tomorrow's forecast -->
    <string name="tomorrow">Tomorrow</string>

    <!-- Date format -->
    <string name="format_full_friendly_date">
        <xliff:g id="month">%1$s</xliff:g>, <xliff:g id="day">%2$s</xliff:g>
    </string>
    <string name="sync_account_type">myweatherdatabase.example.com</string>


</resources>
